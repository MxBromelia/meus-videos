<% if notice %>
<div class='notice' %>
  <p><%= notice %></p>
</div>
<% end %>
<iframe
    width="560"
    height="315"
    src="<%= @video.embed_url %>"
    title="YouTube video player"
    frameborder="0"
    allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture"
    allowfullscreen>
</iframe>