<% if @video.errors.any? %>
    <div class=alert>
        <p><%= @video.errors.count %> Erros Encontrados:</p>
        <ul>
        <% @video.errors.full_messages.each do |error| %>
            <li><%=error%></li>
        <% end %>
        </ul>
    </div>
<% end %>
<%= form_for [@category, @video] do |form| %>
    <%= form.label :url %>
    <%= form.text_field :url %>

    <%= form.label :title %>
    <%= form.text_field :title %>

    <%= form.submit %>
<% end %>